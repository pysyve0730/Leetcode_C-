class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        string result = "";
        for(int i=0;i<strs.size();i++){
            //cout<<strs[i]<<endl;
            for(int j=i+1;j<strs.size();j++){
                for(int k=0;k<strs.size();k++){
                    if(strs[i][k]==strs[j][k]){
                        result.push_back(strs[i][k]);
                    }

                }
            }
            return result;
        }
        return "";
    }
};
